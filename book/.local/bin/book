#!/bin/sh

# BOOK
# Simon Hugh Moore
#
# A password store wrapper to make it into a "bookmark store".

export PASSWORD_STORE_DIR=${BOOK_DIR:-~/.book}
export PASSWORD_STORE_ENABLE_EXTENSIONS=${BOOK_ENABLE_EXTENSIONS:-"true"}
export PASSWORD_STORE_EXTENSIONS_DIR=${BOOK_EXTENSIONS_DIR:-$PASSWORD_STORE_DIR/.extensions}

[ "$1" = "generate" ] && echo "generate has been disables!"; exit 1
pass $@
